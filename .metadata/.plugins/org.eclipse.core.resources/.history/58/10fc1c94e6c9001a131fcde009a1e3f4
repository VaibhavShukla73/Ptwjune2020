package JavaSecession;

public class GetterSetterConcepts {
	
	int age;
	String name;
	int sal;
	
	
	public void setAge(int age) {
		this.age=age;
	}
	
	public int gettAge() {
		return this.age;
	}
	
	
	public void setName(String name) {
		this.name=name;
	}
	
	public String getName() {
		return this.name;
	}
	
	public void setSal(int sal) {
		this.sal=sal;
	}
	
	public int getSal() {
		return this.sal;
	}
	
	
	
	
	
	
	

}
