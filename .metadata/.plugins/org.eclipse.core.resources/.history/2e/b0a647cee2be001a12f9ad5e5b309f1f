package SundayBatch;

public class ArrayConcept {

	public static void main(String[] args) {

		int rollno[]=new int[5];
		
		 rollno[0]=101;
		 rollno[1]=102;
		 rollno[2]=103;
		 rollno[3]=104;
		 rollno[4]=105;
		 rollno[-1]=200;
//		 rollno[5]=106;
//		 rollno[6]=107;
		 
		 System.out.println(rollno[0]);
		 
		 System.out.println(rollno[1]);

		 
		
		
		
		
		
		
		
		
		
		
		
	}

}
