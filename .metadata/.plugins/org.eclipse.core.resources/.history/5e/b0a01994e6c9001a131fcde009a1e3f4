package JavaSecession;

public class TestGetterAndSetter {

	public static void main(String[] args) {

		GetterSetterConcepts g1=new GetterSetterConcepts();
		
		g1.setName("Tom");
		g1.setAge(25);
		g1.setSal(2000);
		
		
		
		System.out.println(g1.getName()+"  "+g1.getSal()+"   "+g1.gettAge());
		
		
		
		
	}

}
