package SundayBatch;

public class LoopConcept {

	public static void main(String[] args) {

		int i = 101;
		do {
			System.out.println("i value is "+i);
			i = i + 1;   //2=2+1;

		} while (i <= 100);// 1<=10   3<=10   101<=100


	
	
	System.out.println("=========================");
	
	
	int j=2;
	while(j<=10) {   //1<=10   2<=10   3<=10
		
		System.out.println("j value is  "+j);   // 
		j=j+2;    //1=1+1;   2    2=2+1 3
		//j++;
		

		
	}
	
	System.out.println("=========================");
	
	
	for(int k=1;k<=10;k++) {
		System.out.println("k value is "+k);
	}
	
System.out.println("=========================");
	
	
	for(int l=1;l<=10;l=l+2) {
		System.out.println("l value is "+l);
	}
	
	System.out.println("=========================");
	
	
for(int m=1;m<=50;m++) {
	
	   if(m%5==0) {  //1%5==0   5%5==0  6%5==0 1   10%5 ==0
		   System.out.println("Hello  "+m);
		   
	   }
	   
	   
	   
	
	
	//15/3  5
	//15%5=0
	//10%2=0
	
	
	
}
	

System.out.println("=========================");

for(int a=1;a<=10;a++) {   //main loop
	
	for(int b=1;b<=10;b++) {  //inner loop
		
		System.out.println("a value"+a+"   "+"b value"+b);
		
	}
	
	
	
	
	
}




	
	
	
	
//	for(;;) {
//		System.out.println("Hi...........");
//	}
	
	
	
	
	
	
	
	}



	
	
	
}

